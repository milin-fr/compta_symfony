{% extends 'base.html.twig' %}

{% block title %}Ajouter un type de travaux{% endblock %}

{% block body %}
    <h1>Ajouter un type de travaux</h1>

{{ form_start(form) }}
    {{ form_label(form.title, 'Type de charges') }}
    {{ form_widget(form.title) }}
    {{ form_label(form.budget, 'Budget') }}
    {{ form_widget(form.budget) }}
    <input class="hidden" type="submit" id="worktype--submit-btn">
{{ form_end(form) }}
<label class="btn btn-primary" for="worktype--submit-btn">Enregistrer</label>

{% endblock %}
